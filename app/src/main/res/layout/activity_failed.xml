<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_gravity="center">

    <ImageButton
        android:id="@+id/btn_home"
        android:layout_width="58dp"
        android:layout_height="55dp"
        android:background="@android:color/transparent"
        android:text="홈으로"
        app:srcCompat="@drawable/group_30"
        android:layout_marginTop="5dp"/>

    <ImageButton
        android:id="@+id/btn_guide"
        android:layout_width="102dp"
        android:layout_height="55dp"
        android:layout_marginTop="680dp"
        android:background="@android:color/transparent"
        android:layout_gravity="center|top"
        android:text="촬영 가이드"
        app:srcCompat="@drawable/group_28" />

    <ImageButton
        android:id="@+id/btn_retry"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="다시 촬영하기"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/group_27"
        android:layout_marginTop="625dp"  />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="506dp"
        android:layout_height="605dp"
        android:layout_gravity="center|top"
        android:layout_marginTop="10dp"
        app:srcCompat="@drawable/group_37" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="93dp"
        android:layout_height="85dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="-5dp"
        app:srcCompat="@drawable/cookie" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="160dp"
        android:layout_height="210dp"
        app:srcCompat="@drawable/group23"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="40dp"/>
    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="160dp"
        android:layout_height="210dp"
        app:srcCompat="@drawable/group24"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="210dp"/>
    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="160dp"
        android:layout_height="210dp"
        app:srcCompat="@drawable/group25"
        android:layout_marginTop="380dp"
        android:layout_marginLeft="210dp"/>
    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="160dp"
        android:layout_height="210dp"
        app:srcCompat="@drawable/group26"
        android:layout_marginTop="380dp"
        android:layout_marginLeft="40dp"/>


</FrameLayout>